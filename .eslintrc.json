    "useVibrate": false,
    "useVirtualList": false,
    "useVModel": false,
    "useVModels": false,
    "useWakeLock": false,
    "useWebNotification": false,
    "useWebSocket": false,
    "useWebWorker": false,
    "useWebWorkerFn": false,
    "useWindowFocus": false,
    "useWindowScroll": false,
    "useWindowSize": false,
    "watchAtMost": false,
    "watchOnce": false,
    "watchWithFilter": false,
    "whenever": false,
    "useNuxt2Meta": false,
    // copy from node_modules/@vue/runtime-core/dist/runtime-core.d.ts
    "defineProps": false,
    "defineEmits": false,
    "defineExpose": false,
    "withDefaults": false,
    // copy from node_modules/unplugin-vue2-script-setup/ref-macros.d.ts
    "$": false,
    "$$": false,
    "$ref": false,
    "$shallowRef": false,
    "$computed": false
  }
}