    "onStartTyping": false,
    "pausableWatch": false,
    "reactify": false,
    "reactifyObject": false,
    "reactiveComputed": false,
    "reactiveOmit": false,
    "reactivePick": false,
    "refDefault": false,
    "syncRef": false,
    "templateRef": false,
    "throttledRef": false,
    "throttledWatch": false,
    "toReactive": false,
    "tryOnBeforeUnmount": false,
    "tryOnMounted": false,
    "tryOnScopeDispose": false,
    "tryOnUnmounted": false,
    "unrefElement": false,
    "until": false,
    "useActiveElement": false,
    "useAsyncQueue": false,
    "useAsyncState": false,
    "useBase64": false,
    "useBattery": false,
    "useBreakpoints": false,
